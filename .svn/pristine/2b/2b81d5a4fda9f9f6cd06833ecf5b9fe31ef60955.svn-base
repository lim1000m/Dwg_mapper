@charset "euc-kr";

/* base */
*{padding:0;margin:0;}
html{font-size:12px;font-family:돋움,Dotum,Arial,AppleGothic,sans-serif;color:#393939;}
h1, h2, h3, h4, h5, h6{font-size:100%;}
ol, ul, li{list-style-type:none;}
form, fieldset, img{border:0;}
input, select, textarea{font-size:12px;color:#333;height:18px;border:1px solid #a9a9ab;vertical-align:middle;}
input[type=radio],input[type=checkbox],input[type=image], input.rc{width:auto !important;height:auto !important;border:none !important;}
input[type=radio],input[type=checkbox], input.rc{margin-top:-2px;*margin-right:-2px;}
input.num3{width:30px !important;text-align:right !important;ime-mode:disabled;}
input.num4{width:40px !important;text-align:right !important;ime-mode:disabled;}
input.cal_ymd{width:80px !important;}
textarea{width:100%;}
.tp_ro{color:#333 !important;border:1px solid #aaa !important;background:#dedede !important;}/*readonly type form*/
img{vertical-align:middle;}
a{text-decoration:none;color:#393939;}
a:active,a:hover{text-decoration:underline;color:#000;}
/*table{border-collapse:collapse;border-spacing:0;}*/
*html .png24 {tmp:expression(setPng24(this));}/*스크립트 선언 후 사용*/

/* 숨김영역 */
.skip{overflow:hidden;position:absolute;left:-2000%;z-index:-1;font-size:0;line-height:0;color:#FFF;}
.skip2{visibility:hidden;overflow:hidden;width:1px;height:1px;font-size:1px;line-height:0;color:#FFF;}

/* iframe 설정 */
iframe{width:100%;height:100%;}

/* etc */
.fr{float:right !important;}
.fl{float:left !important;}
.center{margin:0 auto !important;}
.d_in{display:inline !important;}
.d_bl{display:block !important;}
.d_no{display:none !important;}
.txtl{text-align:left !important;}
.txtr{text-align:right !important;}
.txtc{text-align:center !important;}
.clear{clear:both !important;}
.dummy{margin:-1px;font-size:0;line-height:0;*zoom:1;}
.tinytxt{font-size:1px;line-height:0px !important;}
.txt_nrw{letter-spacing:-1px !important;}
.txtb{font-weight:bold;}
.txt_em{color:OrangeRed;}
.imode{ime-mode:disabled !important;}
.point{cursor:pointer !important;}
.po_a{position:absolute !important;}
.po_r{position:relative !important;}
.nobg{background:none !important;}
.nobg_img{background-image:none !important;}
.nobor{border:0 !important;}
.bor_basic{border:1px solid #a6a6a6;}
.test {border:1px solid Red;}

/* margin&padding */
.m0{margin:0 !important;}
